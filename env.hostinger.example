# Salin ke .env di server Hostinger (folder root project, bukan di public)
# Sesuaikan nilai di bawah dengan kredensial Hostinger Anda.

APP_NAME="Sistem Posyandu"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=Asia/Jakarta
APP_URL=https://posyandukaranggan.com

LOG_CHANNEL=stack
LOG_STACK=single
LOG_LEVEL=error

DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=nama_database_hostinger
DB_USERNAME=user_database_hostinger
DB_PASSWORD=password_database_hostinger

BROADCAST_CONNECTION=log
CACHE_STORE=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true

MAIL_MAILER=log
MAIL_FROM_ADDRESS="noreply@posyandukaranggan.com"
MAIL_FROM_NAME="${APP_NAME}"
